msgid "This field is required."
msgstr "Este campo es obligatorio."

msgid "This field must be unique."
msgstr "Este campo debe ser único."

msgid "The value of this field must be an integer."
msgstr "El valor de este campo debe ser un número entero."

msgid "This field only accepts binary values."
msgstr "Este campo solo admite valores binarios."

msgid "This field must be true or false."
msgstr "Este campo debe ser verdadero o falso."

msgid "Ensure this field has no more than {max_length} characters."
msgstr "Asegúrate de que este campo tenga menos de {max_length} caracteres."

msgid "Enter a valid date."
msgstr "Ingresa una fecha válida."

msgid "Enter a valid date and time."
msgstr "Ingresa una fecha y hora válidas."

msgid "Enter a valid decimal number."
msgstr "Ingresa un número decimal válido."

msgid "Enter a valid duration."
msgstr "Ingresa una duración válida."

msgid "Enter a valid email address."
msgstr "Ingresa una dirección de correo electrónico válida."

msgid "This field must be a valid file."
msgstr "Debe ser un archivo válido."

msgid "Enter a valid file path."
msgstr "La ruta del archivo debe ser válida."

msgid "Enter a valid floating-point number."
msgstr "Ingresa un número decimal válido."

msgid "This field must be a valid image."
msgstr "Debe ser una imagen válida."

